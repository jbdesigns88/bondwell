{
  "name": "bondwell",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "frontend:install": "cd bondwellApp/frontend && npm install",
    "frontend:build": "cd bondwellApp/frontend && npm run build",
    "frontend:dev": "cd bondwellApp/frontend && npm start",
    "backend:install": "cd bondwellApp/backend && npm install",
    "backend:dev": "cd bondwellApp/backend && npm start",
    "start": "concurrently \"npm run frontend:dev\" \"npm run backend:dev\""
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "morgan": "~1.9.1"
  },
  "devDependencies": {
    "concurrently": "^9.2.0"
  }
}
